      "terminalCursor.background": "#D5C4A1",
      "terminalCursor.foreground": "#D5C4A1"
    }
  },
  "workbench.editor.pinnedTabSizing": "shrink",
  "workbench.layoutControl.type": "both",
  "workbench.list.smoothScrolling": true,
  "workbench.startupEditor": "none",
  "yaml.schemas": {
    "file:///home/aliaksandr/.vscode-server/extensions/atlassian.atlascode-2.10.12/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "git-graph.repository.showRemoteBranches": false,
  "gitlens.views.branches.branches.layout": "list",
  "terminal.integrated.scrollback": 5000,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "window.commandCenter": true,
  "terminal.integrated.shellIntegration.history": 10000,
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.env.osx": {
    "HISTFILE": "/Users/aliaksandr/.zsh_history"
  },
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
